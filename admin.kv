<Admin>
    name:"admin"
    MDNavigationLayout:
        ScreenManage:
            Screen:
                MDBoxLayout:
                    orientation: 'vertical'
                    MDTopAppBar:
                        title: "Title"
                        left_action_items: [["menu",lambda x: nav_drawer.set_state("open")]]
                    ScrollView:
                        Adminhelp:
                            orientation: 'vertical'
                            size_hint:1,None
                            height: self.minimum_height
        MDNavigationDrawer:
            id: nav_drawer
