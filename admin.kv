<Admin>
    name:"admin"
    MDNavigationLayout:
        ScreenManage:
            Screen:
                MDBoxLayout:
                    orientation: 'vertical'
                    MDTopAppBar:
                        title: "Welcome Administrator"
                        left_action_items: [["menu",lambda x: nav_drawer.set_state("open")]]
                    ScrollView:
                        Admin_main:
                            orientation: 'vertical'
                            size_hint:1,None
                            height: self.minimum_height
        MDNavigationDrawer:
            id: nav_drawer
            BoxLayout:
                orientation:'vertical'
                Spinner:
                    id: spinner_id
                    size_hint: (.98,.02)
                    text: "Content"
                    values:["Ready Meal","Sales"]

                    on_text: root.spinner_clicked(spinner_id.text)

            # id: nav_drawer1
                Button:
                    id: spinner_id
                    size_hint: (.98,.02)
                    text: "User Data"
                    on_press:          
                        root.manager.transition.direction = "left"
                        root.manager.current = "table"
                Spinner:
                    id: spinner_id
                    size_hint: (.98,.02)
                    text: "Analysis"
                    values:["Raw Materials analysis","Product analysis","Sales analysis"]

                    on_text: root.spinner_clicked(spinner_id.text)


                MDLabel:
                    id: spinner_label
                    size_hint: (.98,.03)
                    text:root.spinner_text
                    font_name:"Lato"
        
                


