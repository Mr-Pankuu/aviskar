<Admin>
    name:"admin"
    on_pre_enter:root.update()
    MDNavigationLayout:
        ScreenManage:
            Screen: 
                MDBoxLayout:
                    orientation: 'vertical'
                    MDTopAppBar:
                        title: "Welcome Administrator"
                        left_action_items: [["menu",lambda x: nav_drawer.set_state("open")]]
                
                    MDFloatLayout:
                        ScrollView:
                            MDFloatLayout:
                                orientation:'vertical'
                                Image:
                                    source:"farmer.png"
                                    size_hint: .12,.38
                                    pos_hint: {"center_x":0.06,"center_y":0.91}

                                MDFillRoundFlatIconButton:
                                    text: "Raw Materials :"
                                    font_name: "Lato"
                                    font_size: "20sp"
                                    pos_hint: {"center_x": .5,"center_y": .91}
                                    on_press:
                                        root.manager.transition.direction = "left"
                                        root.manager.current = "raw_material"
                                    # halign:"center"
                                    

                                MDLabel:
                                    text: "Hello here you can watch the \nanalysis of the raw material ."
                                    font_name: "Lato"
                                    font_size: "20sp"
                                    pos_hint: {"center_x": .6,"center_y": .77}
                                MDFloatLayout:
                                    md_bg_color: rgba(178, 178, 178, 255)
                                    size_hint: 4, .002
                                    pos_hint: {"center_x": .01,"center_y": .67}

                                Image:
                                    source:"ready-meal.png"
                                    size_hint: .12,.38
                                    pos_hint: {"center_x":0.06,"center_y":0.61}
                                MDFillRoundFlatIconButton:
                                    text: "Menu Data :"
                                    font_name: "Lato"
                                    font_size: "20sp"
                                    pos_hint: {"center_x": .5,"center_y": .61}
                                    on_press: 
                                        root.manager.transition.direction = "left"
                                        root.manager.current = "menudata"


                                MDLabel:
                                    text: "Hello here you can watch the \nanalysis of the Menu Data ."
                                    font_name: "Lato"
                                    font_size: "20sp"
                                    pos_hint: {"center_x": .6,"center_y": .47}
                                MDFloatLayout:
                                    md_bg_color: rgba(178, 178, 178, 255)
                                    size_hint: 4, .002
                                    pos_hint: {"center_x": .01,"center_y": .37}
                                Image:
                                    source:"sales.png"
                                    size_hint: .12,.38
                                    pos_hint: {"center_x":0.06,"center_y":0.31}

                                MDFillRoundFlatIconButton:
                                    text: "Sales Data :"
                                    font_name: "Lato"
                                    font_size: "20sp"
                                    pos_hint: {"center_x": .5,"center_y": .31}
                                    on_press:
                                        root.manager.transition.direction = "left"
                                        root.manager.current = "salesdata"
                                    # SalesDataGraph:
                                    #     size_hint: .5, .5
                                MDLabel:
                                    text: "Hello here you can watch the \nanalysis of the Sales Data ."
                                    font_name: "Lato"
                                    font_size: "20sp"
                                    pos_hint: {"center_x": .6,"center_y": .17}
                                

        MDNavigationDrawer:
            id: nav_drawer
            BoxLayout:
                orientation:'vertical'
                spacing: '8dp'
                padding: '8dp'
                MDFillRoundFlatIconButton:
                    text: "YOUR PROFILE"
                    pos_hint: {"center_x": .5}
                    user_font_size: "38sp"
                    on_press:
                        root.manager.transition.direction = "left"
                        root.manager.current = "adminprofile"

                Image: 
                    source: "Menu/man.png"
                    size_hint: .5, .5
                    pos_hint: {"center_x": .5}

                MDLabel:
                    text: root.username
                    size_hint_y: None
                    pos_hint: {"center_x": .8}
                    height: self.texture_size[1] 
                ScrollView: 
                    MDList:
                        OneLineListItem:
                            icon: 'account-plus'
                            text: 'Account Management'
                            on_release:
                                root.manager.transition.direction = "right"
                                root.manager.current = "account"
                        OneLineListItem:
                            text: 'MENU'
                            on_press: 
                                root.manager.transition.direction = "left"
                                root.manager.current = "menudata"

                        OneLineListItem:
                            text: 'USER DATA ANALYSIS'
                            on_press:
                                root.manager.transition.direction = "left"
                                root.manager.current = "userdata"
                        OneLineListItem:
                            text: 'SALES DATA ANALYSIS'
                            on_press:
                                root.manager.transition.direction = "left"
                                root.manager.current = "salesdata"
                        OneLineListItem:
                            text: 'IN OUT DATA ANALYSIS'
                            on_press:
                                root.manager.transition.direction = "left"
                                root.manager.current = "in_out"
                        OneLineListItem:
                            text: 'RAW DATA ANALYSIS'
                            on_press:
                                root.manager.transition.direction = "left"
                                root.manager.current = "raw_material"
                BoxLayout:
                    orientation:'vertical'
                    MDFillRoundFlatIconButton:
                        text: "About"
                        pos_hint: {"center_x": .5}
                        user_font_size: "50sp"
    
                    MDFillRoundFlatIconButton:
                        text: "Logout"
                        icon: "logout"
                        pos_hint: {"center_x": .5}
                        user_font_size: "36sp"
                        on_press:
                            root.manager.transition.direction = "left"
                            root.manager.current = "main"

