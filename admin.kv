<Admin>
    name:"admin"
    MDNavigationLayout:
        ScreenManage:
            Screen:
                MDBoxLayout:
                    orientation: 'vertical'
                    MDTopAppBar:
                        title: "Title"
                        left_action_items: [["menu",lambda x: nav_drawer.set_state("open")]]
                    ScrollView:
                        Admin_main:
                            orientation: 'vertical'
                            size_hint:1,None
                            height: self.minimum_height
        MDNavigationDrawer:
            id: nav_drawer
            BoxLayout:
                orientation:'vertical'
                MDLabel:
                    id: spinner_label
                    text:"nicozero"
                    font_style:"Subtitle1"
                Spinner:
                    id: spinner_id
                    text: "Content"
                    values:["Profile","Data Anylies","Morning Sales","Evening Sales","About"]

                    on_text: root.spinner_clicked(spinner_id.text)