        MDNavigationDrawer:
            id: nav_drawer
            BoxLayout:
                orientation:'vertical'  
                Spinner:
                    id: spinner_id
                    bg_color: rgba(1,1,1,1)
                    size_hint: (.98,.02)
                    text: "Content"
                    values:["Menu","Sales"]
                    on_text: root.spinner_clicked(spinner_id.text)
                            # id: nav_drawer1
                Button:
                    id: spinner_id
                    size_hint: (.98,.02)
                    text: "User-Data"
                    on_press:          
                        root.manager.transition.direction = "left"
                        root.manager.current = "userdatatable"
                Spinner:
                    id: spinner_id
                    size_hint: (.98,.02)
                    text: "Analysis"
                    values:["Raw Materials analysis","Product analysis","Sales analysis"]
                    on_text: root.spinner_clicked(spinner_id.text)

                MDFillRoundFlatIconButton:
                    text: "Manage Accounts"
                    size_hint: (.98,.02)
                    pos_hint: {"center_y": .95}
                    pos_hint: {"center_x": .5}
                    user_font_size: "30sp"
                    theme_text_color: "Custom"
                    text_color: rgba(26, 24, 58, 255)
                    on_press:          
                        root.manager.transition.direction = "left"
                        root.manager.current = "account"
                MDLabel:
                    id: spinner_label
                    size_hint: (.98,.03)
                    text:root.spinner_text
                    font_name:"Lato"
                
                MDFillRoundFlatIconButton:
                    text: "Sign out"
                    # icon: "sign out"
                    pos_hint: {"center_y": .95}
                    pos_hint: {"center_x": .5}
                    user_font_size: "30sp"
                    theme_text_color: "Custom"
                    text_color: rgba(26, 24, 58, 255)
                    on_release:
                        root.manager.transition.direction = "right"
                        root.manager.current = "main"