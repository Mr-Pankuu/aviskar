<NorthIndian>
    name: "north"
    MDBoxLayout:
        orientation: 'vertical' 
        size: 1, 1
        MDTopAppBar:
            title: "              NORTH INDIAN"
            # md_bg_color: [0,0,1,1]
            # left_action_items: [['menu', lambda x :  arrow-left.set_state("open")]]
