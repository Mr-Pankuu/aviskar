<Raw_material>
    name: "raw_material"
    MDNavigationLayout:
        ScreenManage:
            Screen:
                # ScrollView:
                MDIconButton:
                    icon: "arrow-left"
                    pos_hint: {"center_y": .95}
                    user_font_size: "30sp"
                    text_color: rgba(26, 24, 58, 255)
                    on_release:
                        root.manager.transition.direction = "right"
                        root.manager.current = "admin"
                MDLabel:
                    text: "Hello! You are now in a Raw Material Page"
                    font_name: "Lato"
                    font_size: "20sp"
                    size_hint_x: .85
                    pos_hint: {"center_x": .5,"center_y": .94}
                    halign:"center"
                    color: rgba(127,127,127,255)

